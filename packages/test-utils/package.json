{
  "name": "@ttoss/test-utils",
  "version": "1.14.1",
  "description": "This package provides a number of utilities and re-exports for testing using Jest, React Testing Library, and Storybook.",
  "keywords": [
    "Jest",
    "React",
    "React Testing Library",
    "Storybook"
  ],
  "author": "ttoss",
  "contributors": [
    {
      "email": "pedro@arantespp.com",
      "name": "Pedro Arantes",
      "url": "https://arantespp.com/contact"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./faker": {
      "import": "./dist/esm/faker.js",
      "require": "./dist/faker.js"
    },
    "./relay": {
      "import": "./dist/esm/relay.js",
      "require": "./dist/relay.js"
    },
    "./storybook": {
      "import": "./dist/esm/storybook.js",
      "require": "./dist/storybook.js"
    }
  },
  "typesVersions": {
    "*": {
      "faker": [
        "./dist/faker.d.ts"
      ],
      ".": [
        "./dist/index.d.ts"
      ],
      "relay": [
        "./dist/relay.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "storybook.d.ts",
    "storybook.js"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsup",
    "postbuild": "./scripts/post-build.sh",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/jest": "^11.7.1",
    "@storybook/addon-storyshots": "^6.4.14",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.0.0-alpha.6",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "^5.5.9",
    "@types/relay-test-utils": "^6.0.5",
    "faker": "5.5.3",
    "relay-test-utils": "^13.2.0"
  },
  "peerDependencies": {
    "@storybook/client-api": ">=6.0.0",
    "@storybook/react": ">=6.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@ttoss/config": "^1.14.1"
  },
  "gitHead": "2a911f70d465d6bffdd6589a62d17701f1026f01"
}
